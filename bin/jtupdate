#!/bin/bash
MIST=-mist

while [ $# -gt 0 ]; do
    case "$1" in
        -mister)
            MIST=-mister;;
        -mist)
            MIST=-mist;;
        -all | -a)
			MIST="-mist -mister";;
        *)  echo "ERROR: Unknown option $1";
            exit 1;;
    esac
    shift
done

cores="1942 1943 gng commando"

parallel jtcore ::: $cores ::: $MIST